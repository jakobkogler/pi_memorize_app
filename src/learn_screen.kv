#:kivy 1.9.1

<LearnScreen@Screen>:
    LearnScreenManager:
        id: learn_screen_manager


<LearnScreenManager@Carousel>:
    id: carousel
    loop: False

<LearnInfo@FloatLayout>:
    Label:
        size_hint: .8, .8
        pos_hint: {'center_x': .5, 'center_y': .5}
        id: label
        text: root.label_text
